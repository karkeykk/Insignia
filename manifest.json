{
    "manifest_version": 2,
    "name": "Wisestamp Extension",
    "version": "0.1",
    "content_scripts": [
        {
            "matches":["https://mail.google.com/*","<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["background.js"]
    }
}